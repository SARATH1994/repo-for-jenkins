  
pipeline {
    agent { docker 'maven:alpine' } 
    stages {
        stage('Example Build') {
            steps {
                
                sh 'mvn sonar:sonar -Dsonar.host.url=http://13.232.210.96:9000'
                sh 'mvn clean install'
                sh 'mvn clean deploy'
            }
        }
    }
}
