  
pipeline {
    agent any
 tools { 
        maven 'Maven 3.6.2' 
       }
    
    stages {
        stage('Example Build') {
            steps {
            
            sh '''
                  echo "PATH = ${PATH}"
                  echo "M2_HOME = ${M2_HOME}"
                
                mvn sonar:sonar -Dsonar.host.url=http://13.234.226.161:8081
               
                mvn -X clean deploy
                
                '''
            }
        }
    }
}
