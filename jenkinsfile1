  
pipeline {
    agent { docker 'maven:alpine-new' } 
    stages {
        stage('Example Build') {
            steps {
                
                sh 'mvn sonar:sonar -Dsonar.host.url=http://13.234.112.119:8081'
                sh 'mvn clean install'
                sh 'mvn clean deploy'
            }
        }
    }
}
